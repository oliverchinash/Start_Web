<!doctype html>
<html class="no-js">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="keywords" content="易店心" />
    <meta name="description" content="易店心" />
    <link rel="stylesheet" href="@SuperMarket.Core.Globals.SkinPath/css/amazeui.min.css"> 
    <link href="@SuperMarket.Core.Globals.SkinPath/css/basic.css?type=@SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.CssVersion.MemberStyle" rel="stylesheet" />
     <link rel="stylesheet" href="@SuperMarket.Core.Globals.SkinPath/css/relogin.css?type=@SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.CssVersion.MemberStyle"> 
     <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
     <script src="@SuperMarket.Core.Globals.SkinPath/js/jquery-1.11.1.min.js"></script>
    <script src="@SuperMarket.Core.Globals.SkinPath/js/amazeui.min.js"></script>
    <script src="@SuperMarket.Core.Globals.SkinPath/js/jsglobalenum.js?type=@(SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.JsVersion.GlobalCarts)"></script>
    <script src="@SuperMarket.Core.Globals.SkinPath/js/common.js?type=@(SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.JsVersion.GlobalCarts)"></script>
    <script src="@SuperMarket.Core.Globals.SkinPath/js/commoncontrolbind.js?type=@(SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.JsVersion.GlobalCarts)"></script>
    @RenderSection("Head", false)
    <script type="text/javascript">
        var ApplicationPath = "";
        var WebUrl = "/";
        var returnpath = "@Request.Path";
        var returnurl = "@Request["ReturnUrl"]";
        if (returnurl == "") {
            returnurl = "@Request.UrlReferrer";
        }
    </script>
</head>
<body >
    <div style="display:none;">
        <input id="txtPicServerRootUrl" value="@(SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.ImagesServerUrl)" />
        <input id="hidMainCommonDomain" value="@SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.DomainShopping" />
        <input id="hidReturnUrl" value="@ViewBag.ReturnUrl" />
        <input id="hidMainWebUrl" value="@SuperMarket.Core.ConfigCore.Instance.ConfigCommonEntity.MainWebUrl" />
    </div>  
    @RenderBody() 
    @Html.Partial("controls/_BodyBefore")
</body>
</html>
